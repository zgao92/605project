
universe = vanilla
log = log/job_$(Cluster).log
error = error/job_$(Cluster)_$(Process).err
num = $INT(Process,%03d)

executable = ./job.sh

arguments = $(num)
output = output/job_$(Cluster)_$(Process).out

should_transfer_files = YES
when_to_transfer_output = ON_EXIT
transfer_input_files = datadump_s5-$(num).csv

request_cpus = 1
request_memory = 1GB
request_disk = 1GB

queue 22
